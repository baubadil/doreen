<?xml version='1.0' encoding='UTF-8'?>
<DLG>

<!-- Session expired dialog. -->
<div class="modal fade" tabindex="-1" id="expiredSessionDialog">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span class="sr-only">{{L//Close}}</span></button>
        <h1 class="modal-title drn-find-replace-ticket" id="expiredSessionDialog-title">{{L//Session expired}}</h1>
      </div>
      <div class="modal-body">
        <p>{{L//Your session has expired. This means you can no longer save changes you have made to the page. Please back up your changes and reload the page to restart the session. You may have to sign in again.}}</p>
      </div>
      <div class="modal-footer">
        <div class="alert alert-danger hidden drn-error-box" role="alert"><p>Error message</p></div>
        <button type="button" class="btn btn-warning" id="expiredSessionDialog-refresh">{{L//Reload page now}}</button>
        <button type="submit" class="btn btn-primary" id="expiredSessionDialog-submit" autocomplete="off" data-dismiss="modal">{{L//Return to page}}</button>
      </div>
    </div>
  </div>
</div>
</DLG>
